      <input message="tns:getNote"/>
      <output message="tns:getNoteResponse"/>
    </operation>
    <operation name="getObject">
      <documentation></documentation>
      <input message="tns:getObject"/>
      <output message="tns:getObjectResponse"/>
    </operation>
    <operation name="getPhoneAppActivationCode">
      <documentation>Some vendor's mobile app requires an activation code. Use this method to get an activation data. </documentation>
      <input message="tns:getPhoneAppActivationCode"/>
      <output message="tns:getPhoneAppActivationCodeResponse"/>
    </operation>
