    </operation>
    <operation name="getDatacenters">
      <documentation>Retrieve all datacenter locations. SoftLayer's datacenters exist in various cities and each contain one or more server rooms which house network and server infrastructure. </documentation>
      <input message="tns:getDatacenters"/>
      <output message="tns:getDatacentersResponse"/>
    </operation>
    <operation name="getDatacentersWithVirtualImageStoreServiceResourceRecord">
