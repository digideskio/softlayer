    </operation>
    <operation name="getGroups">
      <documentation>Retrieve a location can be a member of 1 or more groups. This will show which groups to which a location belongs.</documentation>
      <input message="tns:getGroups"/>
      <output message="tns:getGroupsResponse"/>
    </operation>
    <operation name="getHardwareFirewalls">
      <documentation></documentation>
      <input message="tns:getHardwareFirewalls"/>
      <output message="tns:getHardwareFirewallsResponse"/>
    </operation>
    <operation name="getLocationAddress">
      <documentation>Retrieve a location's physical address.</documentation>
      <input message="tns:getLocationAddress"/>
      <output message="tns:getLocationAddressResponse"/>
    </operation>
    <operation name="getLocationReservationMember">
      <documentation>Retrieve a location's Dedicated Rack member</documentation>
      <input message="tns:getLocationReservationMember"/>
      <output message="tns:getLocationReservationMemberResponse"/>
    </operation>
    <operation name="getLocationStatus">
      <documentation>Retrieve the current locations status.</documentation>
      <input message="tns:getLocationStatus"/>
      <output message="tns:getLocationStatusResponse"/>
    </operation>
    <operation name="getNetworkConfigurationAttribute">
