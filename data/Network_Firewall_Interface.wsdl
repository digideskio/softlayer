        <soap:header message="tns:SoftLayer_Network_Firewall_InterfaceObjectFilterHeader" part="SoftLayer_Network_Firewall_InterfaceObjectFilter" use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
