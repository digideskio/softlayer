      <input message="tns:getNetworkComponents"/>
      <output message="tns:getNetworkComponentsResponse"/>
    </operation>
    <operation name="getNetworkSpace">
      <documentation>Retrieve identifier to denote whether a VLAN is used for public or private connectivity.</documentation>
      <input message="tns:getNetworkSpace"/>
      <output message="tns:getNetworkSpaceResponse"/>
    </operation>
    <operation name="getNetworkVlanFirewall">
      <documentation>Retrieve the Hardware Firewall (Dedicated) for a network vlan.</documentation>
