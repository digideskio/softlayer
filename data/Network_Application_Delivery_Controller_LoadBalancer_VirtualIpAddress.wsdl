    </operation>
    <operation name="getApplicationDeliveryController">
      <documentation>Retrieve a virtual IP address's associated application delivery controller.</documentation>
      <input message="tns:getApplicationDeliveryController"/>
      <output message="tns:getApplicationDeliveryControllerResponse"/>
    </operation>
    <operation name="getApplicationDeliveryControllers">
      <documentation>Retrieve a virtual IP address's associated application delivery controllers.</documentation>
      <input message="tns:getApplicationDeliveryControllers"/>
      <output message="tns:getApplicationDeliveryControllersResponse"/>
    </operation>
    <operation name="getAvailableSecureTransportCiphers">
      <documentation>Yields a list of the SSL/TLS encryption ciphers that are currently supported on this virtual IP address instance. </documentation>
      <input message="tns:getAvailableSecureTransportCiphers"/>
      <output message="tns:getAvailableSecureTransportCiphersResponse"/>
    </operation>
    <operation name="getAvailableSecureTransportProtocols">
      <documentation>Yields a list of the secure communication protocols that are currently supported on this virtual IP address instance. The list of supported ciphers for each protocol is culled to match availability. </documentation>
      <input message="tns:getAvailableSecureTransportProtocols"/>
      <output message="tns:getAvailableSecureTransportProtocolsResponse"/>
    </operation>
    <operation name="getBillingItem">
      <documentation>Retrieve the current billing item for the load balancer virtual IP. This is only valid when dedicatedFlag is false. This is an independent virtual IP, and if canceled, will only affect the associated virtual IP.</documentation>
      <input message="tns:getBillingItem"/>
      <output message="tns:getBillingItemResponse"/>
    </operation>
    <operation name="getDedicatedBillingItem">
      <documentation>Retrieve the current billing item for the load balancing device housing the virtual IP. This billing item represents a device which could contain other virtual IPs. Caution should be taken when canceling. This is only valid when dedicatedFlag is true.</documentation>
      <input message="tns:getDedicatedBillingItem"/>
      <output message="tns:getDedicatedBillingItemResponse"/>
    </operation>
    <operation name="getHighAvailabilityFlag">
      <documentation>Retrieve denotes whether the virtual IP is configured within a high availability cluster.</documentation>
      <input message="tns:getHighAvailabilityFlag"/>
      <output message="tns:getHighAvailabilityFlagResponse"/>
    </operation>
    <operation name="getIpAddress">
      <documentation></documentation>
      <input message="tns:getIpAddress"/>
      <output message="tns:getIpAddressResponse"/>
    </operation>
    <operation name="getLoadBalancerHardware">
      <documentation></documentation>
      <input message="tns:getLoadBalancerHardware"/>
      <output message="tns:getLoadBalancerHardwareResponse"/>
    </operation>
    <operation name="getManagedResourceFlag">
      <documentation>Retrieve a flag indicating that the load balancer is a managed resource.</documentation>
      <input message="tns:getManagedResourceFlag"/>
      <output message="tns:getManagedResourceFlagResponse"/>
    </operation>
    <operation name="getObject">
      <documentation></documentation>
      <input message="tns:getObject"/>
      <output message="tns:getObjectResponse"/>
    </operation>
    <operation name="getSecureTransportCiphers">
      <documentation>Retrieve the list of security ciphers enabled for this virtual IP address</documentation>
      <input message="tns:getSecureTransportCiphers"/>
      <output message="tns:getSecureTransportCiphersResponse"/>
    </operation>
    <operation name="getSecureTransportProtocols">
      <documentation>Retrieve the list of secure transport protocols enabled for this virtual IP address</documentation>
      <input message="tns:getSecureTransportProtocols"/>
      <output message="tns:getSecureTransportProtocolsResponse"/>
    </operation>
    <operation name="getSecurityCertificate">
      <documentation>Retrieve the SSL certificate currently associated with the VIP.</documentation>
      <input message="tns:getSecurityCertificate"/>
      <output message="tns:getSecurityCertificateResponse"/>
    </operation>
    <operation name="getSecurityCertificateEntry">
      <documentation>Retrieve the SSL certificate currently associated with the VIP. Provides chosen certificate visibility to unprivileged users.</documentation>
      <input message="tns:getSecurityCertificateEntry"/>
      <output message="tns:getSecurityCertificateEntryResponse"/>
    </operation>
    <operation name="getVirtualServers">
      <documentation></documentation>
      <input message="tns:getVirtualServers"/>
      <output message="tns:getVirtualServersResponse"/>
    </operation>
    <operation name="startSsl">
      <documentation>Start SSL acceleration on all SSL virtual services (those with a type of HTTPS). This action should be taken only after configuring an SSL certificate for the virtual IP. </documentation>
      <input message="tns:startSsl"/>
      <output message="tns:startSslResponse"/>
    </operation>
    <operation name="stopSsl">
      <documentation>Stop SSL acceleration on all SSL virtual services (those with a type of HTTPS). </documentation>
      <input message="tns:stopSsl"/>
      <output message="tns:stopSslResponse"/>
    </operation>
    <operation name="upgradeConnectionLimit">
      <documentation>Upgrades the connection limit on the Virtual IP to Address to the next, higher connection limit of the same product. </documentation>
      <input message="tns:upgradeConnectionLimit"/>
      <output message="tns:upgradeConnectionLimitResponse"/>
    </operation>
