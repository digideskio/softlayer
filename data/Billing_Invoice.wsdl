    <operation name="getInvoiceTotalAmount">
      <documentation>Retrieve the total amount of this invoice.</documentation>
      <input message="tns:getInvoiceTotalAmount"/>
      <output message="tns:getInvoiceTotalAmountResponse"/>
    </operation>
    <operation name="getInvoiceTotalOneTimeAmount">
      <documentation>Retrieve the total one-time charges for this invoice. This is the sum of one-time charges + setup fees + labor fees. This does not include taxes.</documentation>
      <input message="tns:getInvoiceTotalOneTimeAmount"/>
      <output message="tns:getInvoiceTotalOneTimeAmountResponse"/>
    </operation>
    <operation name="getInvoiceTotalOneTimeTaxAmount">
      <documentation>Retrieve a sum of all the taxes related to one time charges for this invoice.</documentation>
      <input message="tns:getInvoiceTotalOneTimeTaxAmount"/>
      <output message="tns:getInvoiceTotalOneTimeTaxAmountResponse"/>
    </operation>
    <operation name="getInvoiceTotalPreTaxAmount">
      <documentation>Retrieve the total amount of this invoice. This does not include taxes.</documentation>
      <input message="tns:getInvoiceTotalPreTaxAmount"/>
      <output message="tns:getInvoiceTotalPreTaxAmountResponse"/>
    </operation>
    <operation name="getInvoiceTotalRecurringAmount">
