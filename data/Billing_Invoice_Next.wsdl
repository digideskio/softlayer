        <soap:header message="tns:SoftLayer_Billing_Invoice_NextInitParametersHeader" part="SoftLayer_Billing_Invoice_NextInitParameters" use="encoded" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
